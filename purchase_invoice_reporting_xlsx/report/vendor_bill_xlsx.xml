<?xml version="1.0" encoding="UTF-8"?>

<odoo>
	<record id="action_vendor_bill_xlsx" model="ir.actions.report.xml">
		<field name="name">Export Vendor bill to Excel</field>
		<field name="model">account.invoice</field>
		<field name="type">ir.actions.report.xml</field>
		<field name="report_name">vendor.bill.xlsx</field>
		<field name="report_type">xlsx</field>
		<field name="auto" eval="False"/>
	</record>

	<record id="action_vendor_bill_xlsx_values" model="ir.values">
		<field name="name">Export Vendor bill to Excel</field>
		<field name="key2">client_action_multi</field>
		<field name="value" eval="'ir.actions.report.xml,' +str(ref('action_vendor_bill_xlsx'))"/>
		<field name="model">account.invoice</field>
	</record>
</odoo>
